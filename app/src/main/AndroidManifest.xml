<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.eightball">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".EightBall"
            android:label = "@string/cc_app_name">
            <intent-filter>
                <category android:name = "android.intent.category.LAUNCHER"/>
                <action android:name = "android.intent.action.MAIN"/>v
            </intent-filter>
        </activity>

        <activity
            android:name=".AnswerActivity"
            android:parentActivityName=".EightBall"
            android:label="@string/answer_activity_title">
        </activity>

    </application>

</manifest>
